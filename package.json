{
  "name": "uplink-monitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "nodemon --watch 'server/**/*.ts' --exec ts-node server/server.ts",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "install-services": "node install-server-service.js && node install-frontend-service.js",
    "uninstall-services": "node uninstall-server-service.js && node uninstall-frontend-service.js",
    "build-production": "npm run build && tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "mssql": "^11.0.1",
    "node-windows": "^1.0.0-beta.8",
    "ping": "^0.4.4",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mssql": "^9.1.8",
    "@types/node": "^24.4.0",
    "@types/ping": "^0.4.4",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "systeminformation": "^5.27.9",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "ws": "^8.18.3"
  }
}
